<div class="movie-page">
    <div id="movie-card">
        {{outlet}}
    </div>
    <div>
        <ul class="long-list four-columns movie">
        {{#each model as |movie|}}
            {{#link-to "movies.movie" movie anchor='movie-card' }}
            <li>{{movie}}</li>
            {{/link-to}}
        {{/each}}
        </ul>
    </div>
</div>
